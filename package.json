	"description": "",
	"source": "src/index.html",
	"alias": {
		"components/*": "./src/components/$1",
		"ui/*": "./src/components/ui/$1",
		"hooks/*": "./src/hooks/$1",
		"pages/*": "./src/pages/$1",
		"data/*": "./src/data/$1",
		"contexts/*": "./src/contexts/$1",
		"public/*": "./src/public/$1",
		"utils/*": "./src/utils/$1"
	},
	"homepage": "https://aaronm-git/nespresso-react-parcel-tailwind-app",
	"scripts": {
		"start": "parcel",
		"build": "parcel build",
		"ghPreDeoployBuild": "parcel build --public-url /nespresso-react-parcel-tailwind-app/",
		"clear": "rm -rf .parcel-cache dist",
		"test": "echo \"Error: no test specified\" && exit 1",
		"predeploy": "npm run ghPreDeoployBuild",
		"deploy": "gh-pages -d dist"
	},
